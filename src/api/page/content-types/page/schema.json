{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "seo_title": {
      "type": "string"
    },
    "seo_description": {
      "type": "text"
    },
    "pages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::page.page"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "content.text",
        "content.text-and-image",
        "content.image",
        "content.headline-image",
        "content.card-group",
        "content.news-list"
      ]
    }
  }
}
